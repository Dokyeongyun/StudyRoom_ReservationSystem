<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.studyroomreservationsystem">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="공간정보공학과 스터디룸예약시스템"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">
        <activity android:name=".ChangeInfo"></activity>
        <activity android:name=".ChatActivity" />
        <activity android:name=".ChatTest" />
        <activity android:name=".MyPage_PersonalInfo" />
        <activity android:name=".MyPage_Setting" />
        <activity
            android:name=".ReservationStatus_PopUp"
            android:theme="@android:style/Theme.Dialog" />
        <activity
            android:name=".MyPage_PopUp"
            android:theme="@android:style/Theme.Dialog" />
        <activity android:name=".Register" />
        <activity android:name=".MenuPage" />
        <activity
            android:name=".MainActivity"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>